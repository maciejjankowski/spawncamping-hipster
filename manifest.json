{
  "name": "S-mutas",
  "version": "1.0.2",
  "manifest_version":2,
  "description": "No ads in Spotify. Mute them with this extension. The code is shamelessly snatched from Swyter and repackaged for Chrome",
  "permissions": [
  ],
  "content_scripts": [
    {
      "matches": ["https://play.spotify.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png" 
  }
}
